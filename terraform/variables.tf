variable "aws_region" {
  description = "AWS region to deploy to"
  type        = string
  default     = "us-east-1"
}

variable "environment" {
  description = "Environment name (e.g., dev, prod)"
  type        = string
  default     = "dev"
}

variable "existing_bucket_name" {
  description = "Name of the existing S3 bucket to use for hosting"
  type        = string
} 